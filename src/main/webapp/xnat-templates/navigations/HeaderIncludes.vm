    XNAT.dom = getObject(XNAT.dom||{});
    XNAT.dom.addFormCSRF = function(__form){
        __form = isDefined(__form) ? $$(__form) : jq('form');
        __form.append('<input type="hidden" name="XNAT_CSRF" value="' + csrfToken + '">')
    };

